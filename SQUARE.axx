
	.file	"square.asm"
	.title	"fast table lookup square root function"
	.width	132
	.option	b,d,l,t
	.mnolist

	.include	"sys.equ"
	.include	"macros.h"





 SUBR	square_root

	srl	5,a0
	lmo	a0,a1
	neg	a1
	addk	32,a1

	;a1 is the # of bits of data (0-27)
	clr	a14	;# to shift lft when we're done
	subk	10,a1
	jrle	bgpxgo

	;even after discarding lower 5, we've more than ten bits left.
	;divide by 2, rounding up, and that's half the number we shift
	;right right now, and THE number we shift left later.
	inc	a1
	srl	1,a1
	move	a1,a14
	sll	1,a1
	neg	a1
	srl	a1,a0

bgpxgo	;index
	X8	a0
	addi	bgpxsqroot_tab,a0
	movb	*a0,a0
	sll	32-8,a0
	srl	32-8,a0
	sll	a14,a0
	rets



