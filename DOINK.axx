mode_running

	move	*a13(RUN_TIME),a0		;Used for flying kick
	addk	1,a0
	move	a0,*a13(RUN_TIME)

	move	*a13(USR_VAR1),a0
	jrnz	vghxno_vel

	calla	bounce_off_ropes

	movi	vghxXRUN_VAL,a0

	move	*a13(WALK_FAST),a14
	jrz	vghxno
	movi	vghxXRUN2_VAL,a0
vghxno
	move	@hyper_speed_on,a14
	sll	a14,a0

	move	*a13(MOVE_DIR),a14
	btst	PLAYER_RIGHT_BIT,a14
	jrnz	vghxpos
	neg	a0
vghxpos
	move	a0,*a13(OBJ_XVEL),L

vghxno_vel

	move	*a13(GETUP_TIME),a0		;delay b4 gaining control
	jrnz	vghxout_of_control

	move	*a13(ANIMODE),a0
	btst	MODE_UNINT_BIT,a0
	jrnz	vghxno_interrupt

	move	*a13(STICK_VAL_CUR),a0
	move	*a13(MOVE_DIR),a1
	or	a0,a1
	andi	MOVE_LEFT|MOVE_RIGHT,a1
	cmpi	MOVE_LEFT|MOVE_RIGHT,a1
	jrne	vghxcontinue_run

	;braking with stick.  abort run.

	SETMODE	NORMAL

vghxcontinue_run
vghxno_interrupt

	move	*a13(STICK_VAL_CUR),a0
	clr	a1

	movi	-vghxZDRIFT_VAL,a1
	btst	PLAYER_UP_BIT,a0
	jrnz	vghxsetvel

	movi	vghxZDRIFT_VAL,a1
	btst	PLAYER_DOWN_BIT,a0
	jrnz	vghxsetvel

	clr	a1
vghxsetvel
	move	a1,*a13(OBJ_ZVEL),L


