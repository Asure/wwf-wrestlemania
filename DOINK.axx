******************************************************************************
 SUBRP	dnk_finish_move1

ldplTIMEOUT	.equ	TSEC

ldplend_wait
	SLEEP		TSEC/4
	move		@p1rounds,a0			; Get player 1 rounds
	move		@p2rounds,a1			; Get player 2 rounds
	or		a1,a0				; Put em together
	cmpi		2,a0				; Anybody got 2?
	jrlt		ldplend_wait			; br = no

ldplreset
	clr		a11
	move		a11,@in_finish_move
	SLEEPK		1
	WAITSWITCH_DWN	J_UP,0,ldplreset
	movi		ldplTIMEOUT,a11
	move		a11,@in_finish_move
	WAITSWITCH_DWN	J_DOWN,0,ldplreset
	WAITSWITCH_DWN	J_TOWARD,J_DOWN|J_UP,ldplreset
	WAITSWITCH_DWN	J_TOWARD,J_DOWN|J_UP,ldplreset
	WAITSWITCH_DWN	B_PUNCH,J_ALL,ldplreset

	movi		dnk_finish1_move,a0
	move		a0,*a8(SPECIAL_MOVE_ADDR),L
	DIE
	.endif
	

	.if NUM_DOINK_FINISHES > 1
******************************************************************************
 SUBRP	dnk_finish_move2

apcyTIMEOUT	.equ	TSEC

apcyend_wait
	SLEEP		TSEC/4
	move		@p1rounds,a0			; Get player 1 rounds
	move		@p2rounds,a1			; Get player 2 rounds
	or		a1,a0				; Put em together
	cmpi		2,a0				; Anybody got 2?
	jrlt		apcyend_wait			; br = no

apcyreset
	clr		a11
	move		a11,@in_finish_move
	SLEEPK		1
	WAITSWITCH_DWN	J_UP,0,apcyreset
	movi		apcyTIMEOUT,a11
	move		a11,@in_finish_move
	WAITSWITCH_DWN	J_UP,0,apcyreset
	WAITSWITCH_DWN	J_RIGHT,J_UP,apcyreset
	WAITSWITCH_DWN	J_RIGHT,J_UP,apcyreset
	WAITSWITCH_DWN	B_SPUNCH,J_ALL,apcyreset

	movi		dnk_finish2_move,a0
	move		a0,*a8(SPECIAL_MOVE_ADDR),L
	DIE
	.endif


*******************************************************************************
dnk_hdhold_slam

aulqTIMEOUT	.equ	60

aulqlp0
	SLEEPK	1
aulqlp
	move	*a8(PLYRMODE),a0
	cmpi	MODE_HEADHOLD,a0
	jrz	aulqcont
	cmpi	MODE_HEADHELD,a0
	jrnz	aulqlp0

aulqcont
	clr	a11

;	WAITSWITCH_DWN	J_UP,0,aulqlp

	WAITSWITCH_DWN	J_DOWN,0,aulqlp

	movi	aulqTIMEOUT,a11

	WAITSWITCH_DWN	J_DOWN,0,aulqlp
	WAITSWITCH_DWN	B_SKICK,J_ALL,aulqlp
aulqcontz
	move	*a8(PLYRMODE),a0
	cmpi	MODE_HEADHOLD,a0
	jrz	aulqslam
	cmpi	MODE_HEADHELD,a0
	jrnz	aulqlp0
	move	*a8(I_WILL_DIE),A14
	jrnz	aulqlp0

;Do reversal unless I have been immobilized!
;If not, set immobilize time for opponent and reverse

	move	*a8(IMMOBILIZE_TIME),a14
	jrnz	aulqlp0				;ignore

	CALLA	DO_REVERSAL
	CALLA	DO_REVERSAL_MESS

	;target WHOHITME -- don't hit anyone else
