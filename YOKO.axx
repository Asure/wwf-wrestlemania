nfwtneck_grab
	.word	B_SPUNCH,		J_ALL
	.word	J_TOWARD,		J_REAL_LR
	.word	J_TOWARD,		J_REAL_LR
	.word	8000h | 30		;max
	.long	nfwtscrt_neck

nfwtscrt_neck
	move	*a13(ANIMODE),a0
	btst	MODE_UNINT_BIT,a0
	jrnz	nfwtout

;Don't do it when he is on ground
	calla	get_opp_plyrmode
	cmpi	MODE_ONGROUND,a0
	jrz	nfwtout
	cmpi	MODE_DEAD,a0
	jrz	nfwtout
	cmpi	MODE_HEADHELD,a0
	jrz	nfwtout
;Mode held overhead!
	cmpi	MODE_ATTACHED,a0
	jrz	nfwtout

	move	*a13(LAST_HEADHOLD),a14,L
	move	@PCNT,a0,L
	sub	a14,a0
	cmpi	2*60,a0
	jrge	nfwtok2a

;This head grab is too close (in terms of time) to most recent grab
	.ref	yok_3_fake_hold_anim
	movi	yok_3_fake_hold_anim,a0
	calla	change_anim1a
	rets

nfwtok2a
	move	*a13(CLOSEST_XDIST),a1
	cmpi	80,a1
	jrgt	nfwtstd_grab

	movi	yok_3_head_hold2_anim,a0
	calla	change_anim1a
	rets

nfwtstd_grab
	movi	yok_3_head_hold_anim,a0
	calla	change_anim1a
	rets

