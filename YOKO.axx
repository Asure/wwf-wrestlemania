yok_grab_toss_air

bcwhTIMEOUT	.equ	40

bcwhlp0
	SLEEPK	1
bcwhlp
	clr	a11

	WAITSWITCH_DWN	J_AWAY,0,bcwhlp

	movi	bcwhTIMEOUT,a11

	WAITSWITCH_DWN	J_AWAY,0,bcwhlp
	WAITSWITCH_DWN	B_PUNCH,J_ALL,bcwhlp

	move	*a8(ANIMODE),a0
	btst	MODE_UNINT_BIT,a0
	jrnz	bcwhlp

	move	*a8(PLYRMODE),a0
	cmpi	MODE_HEADHOLD,a0
	jreq	bcwhlp0

;Don't do it when he is on ground
	SWAP	a8,a13
	calla	get_opp_plyrmode
	SWAP	a8,a13
;Don't do it when he is on ground
	cmpi	MODE_ONGROUND,a0
	jrz	bcwhlp
	cmpi	MODE_DEAD,a0
	jrz	bcwhlp
	cmpi	MODE_INAIR,a0
	jrz	bcwhdoit2
	cmpi	MODE_INAIR2,a0
	jrz	bcwhdoit2

	move	*a8(CLOSEST_NUM),a0
	X32	a0
	addi	process_ptrs,a0
	move	*a0,a0,L
	move	*a0(ATTACK_TYPE),a0
	cmpi	AT_LEAPING,a0
	jrz	bcwhdoit2

	move	*a8(CLOSEST_DIST),a0
	cmpi	6ch,a0
	jrgt	bcwhlp

	FACE24	yok,hiptoss_anim
	move	a0,*a8(SPECIAL_MOVE_ADDR),L

	WRSND	W_YOKO,GRABFLING_T1,PUNCH_T2

	SLEEPK	20
	jruc	bcwhlp

bcwhdoit2	FACE24	yok,hiptoss2_anim
	move	a0,*a8(SPECIAL_MOVE_ADDR),L

	WRSND	W_YOKO,GRABFLING_T1,PUNCH_T2

	SLEEPK	20

	jruc	bcwhlp


*******************************************************************************
yok_hdhold_suplex

uofmTIMEOUT	.equ	60

uofmlp0
	SLEEPK	1
uofmlp
;	move	*a8(GETUP_TIME),a0
;	jrnz	uofmlp0
	move	*a8(PLYRMODE),a0
	cmpi	MODE_HEADHOLD,a0
	jrz	uofmcont
	cmpi	MODE_HEADHELD,a0
	jrnz	uofmlp0
uofmcont
	clr	a11

;	WAITSWITCH_DWN	J_UP,0,uofmlp
	WAITSWITCH_DWN	J_DOWN,0,uofmlp

	movi	uofmTIMEOUT,a11

	WAITSWITCH_DWN	J_DOWN,0,uofmlp
	WAITSWITCH_DWN	B_SKICK,J_ALL,uofmlp
	
	move	*a8(PLYRMODE),a0
	cmpi	MODE_HEADHOLD,a0
	jrz	uofmslam
	cmpi	MODE_HEADHELD,a0
	jrnz	uofmlp0
	move	*a8(I_WILL_DIE),A14
	jrnz	uofmlp0

;Do reversal unless I have been immobilized!
;If not, set immobilize time for opponent and reverse

	move	*a8(IMMOBILIZE_TIME),a14
	jrnz	uofmlp0				;ignore

	CALLA	DO_REVERSAL
	CALLA	DO_REVERSAL_MESS

	;target WHOHITME -- don't hit anyone else
	SMRTTGT	a8,WHOHITME

	;immobilize WHOHITME
