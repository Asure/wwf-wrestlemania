	WAITSWITCH_DWN	J_RIGHT,J_UP,sqjrreset
	WAITSWITCH_DWN	J_RIGHT,J_UP,sqjrreset
	WAITSWITCH_DWN	B_SPUNCH,J_ALL,sqjrreset

	movi		yok_finish2_move,a0
