******************************************************************************
 SUBRP	yok_finish_move1

bktvTIMEOUT	.equ	TSEC

bktvend_wait
	SLEEP		TSEC/4
	move		@p1rounds,a0			; Get player 1 rounds
	move		@p2rounds,a1			; Get player 2 rounds
	or		a1,a0				; Put em together
	cmpi		2,a0				; Anybody got 2?
	jrlt		bktvend_wait			; br = no

bktvreset
	clr		a11
	move		a11,@in_finish_move
	SLEEPK		1
	WAITSWITCH_DWN	J_UP,0,bktvreset
	movi		bktvTIMEOUT,a11
	move		a11,@in_finish_move
	WAITSWITCH_DWN	J_DOWN,0,bktvreset
	WAITSWITCH_DWN	J_TOWARD,J_DOWN|J_UP,bktvreset
	WAITSWITCH_DWN	J_TOWARD,J_DOWN|J_UP,bktvreset
	WAITSWITCH_DWN	B_PUNCH,J_ALL,bktvreset

	movi		yok_finish1_move,a0
	move		a0,*a8(SPECIAL_MOVE_ADDR),L
	DIE
	.endif
	

	.if NUM_YOKO_FINISHES > 1
