greuTIMEOUT	.equ	60

greulp0
	SLEEPK	1
greulp
	move	*a8(PLYRMODE),a0
	cmpi	MODE_HEADHOLD,a0
	jrz	greucont
	cmpi	MODE_HEADHELD,a0
	jrnz	greulp0
greucont
	clr	a11

	WAITSWITCH_DWN	J_DOWN,0,greulp

	movi	greuTIMEOUT,a11

	WAITSWITCH_DWN	J_DOWN,0,greulp
	WAITSWITCH_DWN	B_SKICK,J_ALL,greulp
	
	move	*a8(PLYRMODE),a0
	cmpi	MODE_HEADHOLD,a0
	jrz	greuslam
	cmpi	MODE_HEADHELD,a0
	jrnz	greulp0
	move	*a8(I_WILL_DIE),A14
	jrnz	greulp0


	move	*a8(IMMOBILIZE_TIME),a14
	jrnz	greulp0				;ignore

	CALLA	DO_REVERSAL
	CALLA	DO_REVERSAL_MESS

	;target WHOHITME -- don't hit anyone else
	SMRTTGT	a8,WHOHITME

	;immobilize WHOHITME
	move	*a8(WHOHITME),a0,L
	jruc	greuis_reversal

greuslam
	move	*a8(IMMOBILIZE_TIME),a14
	jrnz	greulp0				;ignore

	movi	42,a10
	CREATE	MESSAGE_PID,BONUS_MESS

	;target WHOIHIT -- don't hit anyone else
	SMRTTGT	a8,WHOIHIT

	;immobilize WHOIHIT
	move	*a8(WHOIHIT),a0,L

greuis_reversal
	movk	30,a14
	move	a14,*a0(IMMOBILIZE_TIME)

	CALLA	FIND_AND_KILL_ENDLESS

	movi	yok_vsuplex_anim,a14
	move	a14,*a8(SPECIAL_MOVE_ADDR),L

